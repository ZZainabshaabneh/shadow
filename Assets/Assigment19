using UnityEngine;

public class KineticSculpture : MonoBehaviour
{
    public float speed = 1.0f;
    float x = 0.02f;
    public Vector3 vector = new Vector3(1, 1, 1);
    void Start()
    {
        
    }
    void Update()
    {
    
        transform.position = transform.position + new Vector3(x*speed, 0, 0);
    }
}
